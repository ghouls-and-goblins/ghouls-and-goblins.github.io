{
    "name": "ghouls-goblins-hof",
    "version": "0.1.0",
    "homepage": "https://ghouls-and-goblins.github.io/",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
    	"@babel/polyfill": "7.6.0",
        "@reach/router": "1.2.1",
        "axios": "0.19.0",
        "classnames": "2.2.6",
        "core-js": "3.3.3",
        "node-sass": "4.12.0",
        "prop-types": "15.7.2",
        "react": "16.10.2",
        "react-dom": "16.10.2",
        "react-helmet": "5.2.1",
        "react-intl": "3.3.2",
        "react-scripts": "3.2.0"
    },
    "scripts": {
        "start": "concurrently \"react-scripts start\" \"npm run watch-css\"",
        "build": "npm run scss && react-scripts build",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -b master -d build",
        "lint": "eslint . --ignore-path .gitignore",
        "pa11y": "node pa11y.js",
        "scss": "concurrently \"node-sass -o public src/scss/pages/ghouls--1.scss\" \"node-sass -o public src/scss/pages/ghouls--contrast.scss\" \"node-sass -o public src/scss/pages/ghouls--dark.scss\" \"node-sass -o public src/scss/pages/ghouls--high-contrast-dark.scss\" \"node-sass -o public src/scss/pages/ghouls--high-contrast-light.scss\" \"node-sass -o public src/scss/pages/ghouls--keyboard.scss\"",
        "watch-css": "nodemon -e scss -x \"npm run scss\""
    },
    "devDependencies": {
        "concurrently": "5.0.0",
        "eslint": "6.5.1",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-react": "7.16.0",
        "gh-pages": "2.1.1",
        "nodemon": "1.19.4",
        "pa11y": "5.3.0",
        "pa11y-reporter-html": "1.0.0",
        "react-scripts": "3.2.0",
        "stylelint": "11.1.1"
    }
}
